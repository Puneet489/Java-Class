class a{
    public int num;
    a(int x, int y){
        int num1 = x;
        int num2 = y;
        System.out.println("The sum of the two number is "+(num1+num2));
    }
    
    num = 5;
    System.out.println("This is a instance variable "+ num);
}

class b{
    public static void main(String args[]){
        a a1 = new a(4,5);
        
    }
}
